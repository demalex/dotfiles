# ~/.gitconfig
#
# git(1) configuration file

[alias]
  ci = commit
  co = checkout
  st = status
  discard = checkout -- 
  unstage = reset HEAD

[color]
  ui = auto

[core]
  excludesfile = ~/.gitignore_global

[credential]
  helper = osxkeychain

[user]
  name = Alex Demishkevych
  email = demalex@me.com
